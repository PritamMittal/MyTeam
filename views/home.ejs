<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <!-- importing all the stylesheets and scripts -->
  <link href="home.css" rel="stylesheet" />
  <link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
    integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous" />
  <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
    integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous">
  </script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
    integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous">
  </script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"
    integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous">
  </script>
  <script src="/socket.io/socket.io.js"></script>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <script src="https://kit.fontawesome.com/c939d0e917.js"></script>
  <link
    href="data:image/x-icon;base64,"
    rel="icon" type="image/x-icon" />
  <title>Video Conferencing Platform</title>
</head>

<body style="background-color: #ebf2f8;">
  <div class="home__heading">

    <h3 class="hh" data-text="BringingFolksTogether..."><em>BringingFolksTogether...</em></h3>
    <h1 style="margin-right:-351px;color: white;">My Team</h1>
    <img
      src="data:image/png;base64," />
  </div>
  <div class="container login-container ">
    <div class="row">
      <div class="col-md-6 login-form-1 bigbox">
        <h3>My Teams</h3>
        <!-- Create new room -->
        <div class="create__meeting mx-auto form form-group">
          <a href="/call" id="meeting-href">
            <button class="btn btn-success" style="background-color: #f05837;border-color:#f05837;;">
              Create new Room
              <i class="fa fa-plus ml-3 mr-0" aria-hidden="true"></i>
            </button>
          </a>
        </div>
        <!-- Join exisiting room -->
        <div>
          <div class="meeting-form form-group">
            <input style="width: 51%; " id="meeting-number" type="text" class="form-control"
              placeholder="Enter Teams ID" />
          </div>
          <a href="">
            <button class="btn btn-danger mt-2" type="button" onclick="return joinMeeting()"
              style="margin-left: 190px ;width: 30%;">
              Join Room
            </button>
          </a>
        </div>
      </div>

      <div class="col-md-6 login-form-1 bigbox" style="background-color: #f05837;">
        <h3>My Teams Jr.</h3>
        <!-- Create new room -->
        <div class="create__meeting mx-auto form form-group">
          <a href="/jrCall" id="meeting-href1">
            <button class="btn btn-success"
              style="background-color:rgba(39, 42, 44, 0.95);border-color:rgba(39, 42, 44, 0.95); text-align: center;">
              Create new Room
              <i class="fa fa-plus ml-3 mr-0" aria-hidden="true"></i>
            </button>
          </a>
        </div>
        <!-- Join exisiting room -->
        <div>
          <div class="meeting-form form-group">
            <input style="width: 51%; " id="meeting-numberJr" type="text" class="form-control"
              placeholder="Enter Teams ID" />
          </div>
          <a href="">
            <button class="btn btn-danger mt-2" type="button" onclick="return joinMeetingJr()"
              style="margin-left: 190px ;width: 30%;">
              Join Room
            </button>
          </a>
        </div>
      </div>
    </div>
  </div>
  <!-- script for joining the exisiting room   -->
  <script>
    const joinMeeting = () => {
      var meetingId = $("#meeting-number").val();
      console.log(meetingId);
      url = `https://my-team-prof-junior.herokuapp.com/${meetingId}`;
      console.log(url);
      location.replace(url);
      return false;
    };
  </script>
  <!-- script for joining the exisiting junior room   -->

  <script>
    const joinMeetingJr = () => {
      var meetingId = $("#meeting-numberJr").val();
      console.log(meetingId);
      url = `https://my-team-prof-junior.herokuapp.com/jrCall/${meetingId}`;
      console.log(url);
      location.replace(url);
      return false;
    };
  </script>

</body>

</html>